/*======================================
=          Cart digital popup          =
======================================*/

.cart-digital-popup-wrapper {
  position: fixed;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
  background-color: rgba(0, 0, 0, 0.5);
  z-index: 10000;
  transition: all .3s;

  &.cart-digital-popup-wrapper--hidden {
    opacity: 0;
    visibility: hidden;
  }
}

.cart-digital-popup {
  max-width: 90%;
  background: #FFF;
  border-radius: 8px;
  left: 50%;
  top: 50%;
  transform: translate(-50%, -50%);
  overflow: hidden;
  position: absolute;
  width: 600px;
}

.cart-digital-popup-header {
  position: relative;
  padding: 10px 20px;
  background: #eee;
}

.cart-digital-popup-content {
  padding: 20px;

  .digital-art-product {
    display: flex;
    margin-bottom: 20px;
  }

  .digital-art-product-image {
    margin-right: 20px;
  }

  p {
    font-size: 14px;
    text-align: center;
