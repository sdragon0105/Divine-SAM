  }
}

.product-form__file {
  position: relative;
  overflow: hidden;
  cursor: pointer;

  &:hover,
  &:focus {
    label {
      background-color: $color-btn-primary-focus;
    }
  }

  label {
    color: #fff;
    background-color: $color-btn-primary-bg;
    border-radius: 5px;
    display: flex;
    align-items: center;
    justify-content: center;
    height: 44px;
    line-height: 40px;
    padding: 0 30px;
    transition: all 0.3s ease;

    img {
      margin-right: 10px;
    }
  }

  input[type=file] {
    font-size: 100px;
    position: absolute;
    left: 0;
    top: 0;
    opacity: 0;
    cursor: pointer;
  }

  .uploaded-image {
    position: relative;
    display: none;
    padding: 15px 40px;

    &.file-uploaded {
      display: block;
    }

    .close {
      position: absolute;
      right: 0;
      width: 30px;
      height: 30px;
      background-color: #dbdbdb;
      text-align: center;
      line-height: 30px;
      font-size: 30px;
      display: block;
      color: #fff;
    }

    .image-wrapper {
      border: 4px solid #dbdbdb;
    }
  }
}

