    width: 100%;
    margin-bottom: 20px;
  }

  .support-link {
    background-color: transparent;
    border-color: $color-btn-primary-bg;
    border-radius: 5px;
    color: $color-btn-primary-bg;
    margin-bottom: 20px;
    width: 100%;

    img {
      margin-right: 5px;
    }
