.farbtastic {
  position: relative;
}
.farbtastic * {
  position: absolute;
  cursor: crosshair;
}
.farbtastic, .farbtastic .wheel {
  width: 195px;
  height: 195px;
}
.farbtastic .color, .farbtastic .overlay {
  top: 47px;
  left: 47px;
  width: 101px;
  height: 101px;
}
.farbtastic .wheel {
  background: url({{ "wheel.png" | asset_url }}) no-repeat;
  width: 195px;
  height: 195px;
}
.farbtastic .overlay {
  background: url({{ "mask.png" | asset_url }}) no-repeat;
}
.farbtastic .marker {
  width: 17px;
  height: 17px;
  margin: -8px 0 0 -8px;
  overflow: hidden; 
  background: url({{ "marker.png" | asset_url }}) no-repeat;
}

.color-picker-wrapper {
  display: flex;
  max-width: 650px;
  margin-left: auto;
  margin-right: auto;
  margin-bottom: 40px;
  align-items: center;
  background-color: #ffffff;

  .color-picker-preview,
  .color-picker {
    position: relative;
    width: 50%;
  }

  .color-picker-preview {
    padding: 25px;
    padding-top: 200px;
  }

  .color-picker {
    padding: 0 25px;
    display: flex;
    justify-content: center;
    flex-wrap: wrap;
  }

  #colorpicker {
    width: 100%;
    text-align: center;
    display: flex;
    justify-content: center;
    margin-bottom: 10px;
  }
}

/*======================================
=        Hero image with text          =
======================================*/
.hero-image-with-text {
  .hero-image-with-text__image {
    display: block;
    position: relative;
    background-size: cover;
    background-repeat: no-repeat;
    background-position: center;
  }

  .hero-image-with-text__content {
    display: block;
    margin-left: auto;
    margin-right: auto;
    position: relative;
    padding: 30px 15px 10px 15px;
  }

  .hero-image-with-text__content .section-header {
    margin: 0;
    padding: 0;
    max-width: 520px;
  }

  .hero-image-with-text__content .section-header >.subtitle {
    font-weight: 500;
    font-size: $font-size-base;
    color: $color-body-text;
    display: inline-block;
    border-bottom: 1px solid $color-body-text;
    padding-bottom: 5px;
    margin: 0 0 15px 0;
  }

  .hero-image-with-text__content .section-header > p {
    margin-top: 15px;
  }

  .hero-image-with-text__image--mobile {
    .mobile-image {
      display: block;
    }

    .desktop-image {
      display: none;
    }
  }

  @media (min-width: 768px) {
    display: flex;
    align-items: center;
    justify-content: space-between;

    .hero-image-with-text__image,
    .hero-image-with-text__content {
      width: 50%;
      vertical-align: top;
      display: inline-block;
    }

    .hero-image-with-text__content {
      padding: 60px 30px;
    }

    .hero-image-with-text__image--mobile {
      .mobile-image {
        display: none;
      }

      .desktop-image {
        display: block;
      }
    }
  }

  @media (min-width: 1025px) {
    &.hero-image-with-text--right .hero-image-with-text__content {
      order: -1;
    }

    .hero-image-with-text__content {
      display: flex;
      justify-content: center;
      padding-left: 50px;
      padding-right: 50px;
      padding-top: 80px;
      padding-bottom: 80px;
    }

    .hero-image-with-text__content .section-header > .subtitle {
      margin-bottom: 20px;
    }

    .hero-image-with-text__content .section-header > p {
      margin-top: 25px;
    }

    .hero-image-with-text__content .section-header > .btn {
      margin-top: 35px;
      min-width: 200px;
    }
  }
}

