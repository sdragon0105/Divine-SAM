/*======================================
=    Custom Image With Text Overlay    =
======================================*/

.custom-image-with-text-overlay {
  font-size: 0;
  display: block;
  position: relative;
  overflow: auto;
  white-space: nowrap;
  padding-left: 15px;
  padding-bottom: 5px;

  .custom-image-with-text-overlay__item {
    vertical-align: top;
    font-size: 18px;
    margin-bottom: 20px;
  }

  .image-with-text-overlay {
    width: unset;
  }

  .image-with-text-overlay__content {
    text-align: left;
    top: unset;
    bottom: 0;
    transform: unset;
  }

  .image-with-text-overlay__image:before {
    padding-bottom: 100%;
  }

  .subtitle {
    color: #FFFFFF;
  }

  @media (min-width: 768px) {
    overflow: visible;
    white-space: normal;
    padding-left: 0;
    padding-bottom: 0;
    margin-left: -15px;
    margin-right: -15px;

    .custom-image-with-text-overlay__item {
      display: inline-block;
      width: 50%;
      padding-left: 15px;
      padding-right: 15px;
      margin-right: 0;
      margin-bottom: 40px;
    }
  }
}

