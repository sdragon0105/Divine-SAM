      display: block;
      text-align: center;
      margin: 0 auto;
      height: 44px;
      width: 44px;
      margin-top: 20px;
      margin-bottom: 20px;
      &:after {
        position: relative;
        background: #000;
        height: 44px;
        width: 44px;
        display: table-cell;
        border-radius: 50%;
        counter-increment: my-sec-counter;
        content: counter(my-sec-counter);
        color: #fff;
        font-size: 20px;
        font-weight: 700;
        vertical-align: middle;
      }
    }
    .title {
      margin-top: 20px;
    }
    .cta-link {
      margin-top: 20px;
      color: #ff97ac;
      font-size: 18px;
      font-weight: 700;
      text-transform: uppercase;
    }
  }

  @media screen and (max-width: 767px) {
    .item {
      display: block;
      width: 100%;
      margin-bottom: 20px;
    }
  }
}

