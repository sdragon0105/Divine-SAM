  counter-reset: my-sec-counter;
  .items {
    font-size: 0;
    display: block;
    white-space: nowrap;
    width: auto;
    padding: 0;
    min-width: 100%;
    margin-left: -15px;
    margin-right: -15px;
  }
  .item {
    width: 33.3333333%;
    margin-right: 0;
    padding-left: 15px;
    padding-right: 15px;
    white-space: normal;
    vertical-align: top;
    display: inline-block;
    font-size: 18px;
    .number {
