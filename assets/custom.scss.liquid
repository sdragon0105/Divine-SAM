/*======================================
=                Support               =
======================================*/

.page-support {
  text-align: center;

  .support-content,
  .support-footer {
    padding: 40px 0;
  }

  .support-content {
    border-bottom: 1px solid $color-border;

    .title {
      margin-bottom: 15px;
    }

    .subtitle {
      font-size: 1.5rem;
      margin: auto;
    }
  }

