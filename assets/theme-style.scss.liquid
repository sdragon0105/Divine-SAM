    .search-bar input::-moz-placeholder {
        color: lighten($color-search-bar, 30%);
    }

    .search-bar input:-ms-input-placeholder {
        color: lighten($color-search-bar, 30%);
    }

    .search-bar input::-ms-input-placeholder {
        color: lighten($color-search-bar, 30%);
    }

    .search-bar button {
        border: none;
        padding: 0px 8px;
        width: 40px;
        height: 40px;
        line-height: 40px;
        box-shadow: none;
        color: $color-search-bar;
        background-color: transparent;
