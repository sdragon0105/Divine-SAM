{% if settings.typography_type == 'custom_font' %}
    @font-face {
      font-family: 'Coolvetica';
      src: url('Coolvetica.eot');
      src: url('Coolvetica.eot#iefix') format("embedded-opentype"),
           url('Coolvetica.woff2') format("woff2"),
           url('Coolvetica.woff') format("woff"),
           url('Coolvetica.ttf') format("truetype"),
           url('Coolvetica.svg') format("svg");
      font-weight: normal;
      font-style: normal;
    }

    $font-stack-header: "Coolvetica", {{ header_font.fallback_families }};
    $font-weight-header: 500;

    $font-stack-body: "Jost*", {{ base_font.fallback_families }};
    $font-weight-body: 400;

    $font-stack-heading: "Coolvetica", {{ header_font.fallback_families }};
    $font-weight-heading: 700;

    $font-line-height: 28px;
{% else %}

    $font-stack-header: {{ header_font.family }}, {{ header_font.fallback_families }};
    $font-weight-header: {{ header_font.weight }};

    $font-stack-body: {{ base_font.family }}, {{ base_font.fallback_families }};
    $font-weight-body: {{ base_font.weight }};

    $font-stack-heading: {{ heading_font.family }}, {{ base_font.fallback_families }};
