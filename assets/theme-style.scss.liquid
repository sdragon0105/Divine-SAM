    .breadcrumb__item {
        padding: 5px 0;
        line-height: 1;
        vertical-align: top;
        display: inline-block;
        text-transform: capitalize;
        font-size: $font-size-base;
        color: $color-breadcrumb-text;
    }

    .breadcrumb__item .icon {
