    display: block !important;
    visibility: visible !important;
}

@mixin animation($animation) {
    @include prefix(animation, #{$animation}, moz o webkit spec);
}

@mixin keyframes($name) {
    @-webkit-keyframes #{$name} {
        @content;
    }
    @-moz-keyframes #{$name} {
        @content;
    }
    @-ms-keyframes #{$name} {
        @content;
    }
    @keyframes #{$name} {
