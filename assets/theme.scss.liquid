/* Arrows */

.slick-prev,
.slick-next {
    position: absolute;
    display: block;
    height: 20px;
    width: 20px;
    line-height: 0px;
    font-size: 0px;
    cursor: pointer;
    background: transparent;
    color: transparent;
    top: 50%;
    -webkit-transform: translate(0, -50%);
    -ms-transform: translate(0, -50%);
    transform: translate(0, -50%);
    padding: 0;
    border: none;
    z-index: 10;
    svg {
        width: 100%;
        height: 100%;
        color: $color-heading-text;
        fill: $color-heading-text;
        stroke: $color-heading-text;
    }
    &:hover, &:focus {
        background: transparent;
        color: transparent;
    }
    &.slick-disabled {
        opacity: $slick-opacity-not-active;
    }
    @media (min-width: 1025px) {
        &.slick-arrow--large {
            height: 30px;
            width: 30px;
        }
    }
}

.slick-prev {
    left: 0px;
    @media (min-width: 1025px) {
        &.slick-arrow--large {
            left: -15px;
        }
    }
    @media (min-width: 1600px) {
        &.slick-arrow--large {
            left: -30px;
        }
    }
    @media (min-width: 1920px) {
        &.slick-arrow--large {
            left: -60px;
        }
    }
}

.slick-next {
    right: 0px;
    @media (min-width: 1025px) {
        &.slick-arrow--large {
            right: -15px;
        }
    }
    @media (min-width: 1600px) {
        &.slick-arrow--large {
            right: -30px;
        }
    }
    @media (min-width: 1920px) {
        &.slick-arrow--large {
            right: -60px;
        }
    }
}

/* Dots */
.slick-dotted.slick-slider {
    margin-bottom: 30px;
}

.slick-dots {
    margin: 0;
    padding: 0;
    left: 0;
    right: 0;
    bottom: -30px;
    display: block;
    list-style: none;
    text-align: center;
    position: absolute;
    li {
        padding: 0;
        width: auto;
        height: auto;
        margin: 0 5px;
        display: inline-block;
        vertical-align: middle;
        button {
            width: 8px;
            height: 8px;
            padding: 0;
            opacity: 1;
            font-size: 0;
            border-radius: 50%;
            display: inline-block;
            vertical-align: middle;
            border: 1px solid #bdbdbd;
            background-color: #bdbdbd;
            transition: all 0.3s ease;
            &:hover {
                opacity: 1;
                border-color: #8f8f8f;
                background-color: transparent;
            }
        }
        &.slick-active button {
            border-color: #8f8f8f;
            background-color: transparent;
        }
    }
    @media (min-width: 551px) {
        li {
            margin: 0 6px;
            button {
                width: 10px;
                height: 10px;
            }
        }
    }
}


