/*================ Modals ================*/
.modal {
    top: 0;
    left: 0;
    right: 0;
    bottom: 0;
    opacity: 0;
    display: none;
    overflow: hidden;
    position: fixed;
    transition: all 0.3s ease;
    background-color: rgba(0,0,0,0.5);
    &.modal--is-active {
        .modal-dialog {
            transition-delay: 0.3s;
            -webkit-transform: translate(0,0);
            transform: translate(0,0);
        }
    }
}

#LoginModal {
    height: 100vh;
    background-color: rgba(0,0,0,0.9);
    .modal__inner {
        height: auto;
        max-width: 40rem;
        width: 100%;
        padding-left: 15px;
        padding-right: 15px;
    }
    .password-modal__content {
        padding: 30px 15px;
        position: relative;
        background-color: #ffffff;
    }
    .form-single-field + .password-content {
        margin-top: 15px;
    }
    @media (min-width: 1025px) {
        .password-modal__content {
            padding: 50px 30px;
            background-color: #ffffff;
        }
    }
}

#loading-modal {
    background-color: transparent;
    .loading-modal-content {
        width: 100%;
        text-align: center;
        padding-bottom: 100px;
        .loading {
            width: 150px;
            height: 150px;
        }
    }
}

.modal--is-active {
    opacity: 1;
    display: block;
    overflow: visible;
}

.modal.modal--is-active {
    opacity: 1;
    display: flex;
    overflow: visible;
    align-items: center;
    justify-content: center;
}

.modal__inner {
    @include prefix(transform-style, preserve-3d, moz webkit spec);
    height: 100%;
}

.modal__close {
    border: 0;
    padding: 6px 9px;
    font-size: 16px;
    position: absolute;
    top: 0;
    right: 0;
    z-index: 2;
    .icon {
        font-size: em(20);
    }
}

.modal-content {
    border: none;
    border-radius: 0;
    background-clip: initial;
}

.modal-header {
    padding-left: 15px;
    padding-right: 15px;
    .close {
        padding: 0;
        font-size: 30px;
        line-height: 22px;
        margin: 0;
    }
    @media (min-height: 768px) {
        padding-left: 30px;
        padding-right: 30px;
    }
    @media (min-width: 1400px) {
        padding-left: 50px;
        padding-right: 50px;
    }
}

.modal-header.text-center {
    display: block;
    position: relative;
}

.modal-header.text-center .close {
    top: 17px;
    right: 14px;
    position: absolute;
}

.modal-body {
    max-height: 80vh;
    overflow: auto;
    padding: 15px;
    &::-webkit-scrollbar-track {
        background-color: #f4f4f4;
    }
    &::-webkit-scrollbar {
        width: 5px;
        height: 5px;
        background-color: #f4f4f4;
    }
    &::-webkit-scrollbar-thumb {
        background-color: #000000;
    }
    @media (min-height: 768px) {
        padding: 30px;
    }
    @media (min-width: 1025px) {
        max-height: 620px;
    }
    @media (min-width: 1400px) {
        padding: 50px;
        max-height: 735px;
    }
}

.modal-body.padding-0 {
    padding: 0;
}

.modal-title {
    margin-bottom: 0;
    font-weight: 700;
    text-transform: capitalize;
}

.modal-footer .btn {
    @media (min-width: 551px) {
        min-width: 130px;
    }
}

.modal-dialog {
    @media (min-width: 768px) {
        &.modal-lg {
            max-width: 700px;
        }
    }
    @media (min-width: 1025px) {
        &.modal-lg {
            max-width: 970px;
        }
    }
}

.modal-open .page-container {
    z-index: auto;
}

.modal-open .modal {
    opacity: 1;
    background-color: transparent;
}

.modal-open .modal.fade {
    opacity: 1 !important;
}

.filters-toolbar__input-title .title {
    font-size: $font-size-base;
}

.filters-toolbar__input-title .icon {
    color: $color-heading-text;
    fill: $color-heading-text;
    stroke: $color-heading-text;
}



.price {
    display: flex;
    flex-direction: column;
    align-items: flex-start;
    margin-top: 0;
    margin-bottom: 0;

    dl {
        margin-top: 0;
    }
    dd {
        margin: 0 0.5em 0 0;
    }
}

.price--unavailable {
    visibility: hidden;
}

.price__regular {
    display: block;
}

.price--on-sale {
    .price__regular, .price__availability {
        display: none;
    }
    .price-item--regular {
        text-decoration: line-through;
        color: $color-price-presale;
    }
    .price-item--sale {
        color: $color-price-sale;
    }
}

.price__availability {
    display: none;
}

.price--sold-out {
    .price__availability {
        display: block;
    }
    .price__regular, .price__sale, .price__unit {
        display: none;
    }
}

.price__sale {
    display: none;

    .price--on-sale & {
        display: flex;
    }
}

.price__vendor {
    color: $color-body-text;
    font-size: 0.9em;
    font-weight: $font-weight-body;
    text-transform: uppercase;
    letter-spacing: 1px;
    margin: 5px 0 10px;
    width: 100%;
    @include flex-basis(100%);
}

.price__unit {
    @include flex-basis(100%);
    display: none;

    .price--unit-available & {
        display: block;
    }
}


.price-item {
    font-weight: 400;
    color: $color-price;
}

.price-item--regular {
    .price--on-sale & {
        text-decoration: line-through;
