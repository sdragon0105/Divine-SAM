.spr-icon-star-empty.spr-icon-star-hover:before {
    content: "\2605" !important;
}

.spr-form-review-rating a {
    line-height: 1;
}

.product-card__price {
    line-height: 1;
    position: relative;
    margin: 0 0 0px;
    visibility: visible;
  text-align:center;
    .price-item {
        line-height: 1;
        font-weight: 400;
        display: inline-block;
        vertical-align: middle;
        font-size: $font-size-base;
        color: $color-price;
        + .price-item {
            margin-left: 2px;
        }
    }
    &.price--on-sale {
        .price-item--regular {
            text-decoration: line-through;
            color: $color-price-presale;
        }
        .price-item--sale {
            color: $color-price-sale;
        }
    }
}

.product-card_compare {
    display: none;
    input {
        display: none;
        &:not(:checked) + label:after {
            opacity: 0;
            transform: scale(0) rotate(-45deg)
        }
        &:checked + label:after {
            opacity: 1;
            transform: scale(1) rotate(-45deg);
        }
    }
    label {
        font-weight: 400;
        padding-left: 20px;
        position: relative;
        color: $color-body-text;
        text-transform: capitalize;
        &:before {
            content: "";
            left: 0;
            top: 1px;
            width: 13px;
            height: 13px;
            position: absolute;
            border: 1px solid #dbdbdb;
            background-color: #ffffff;
        }
        &:after {
            content: '';
            width: 9px;
            height: 5px;
            position: absolute;
            top: 4px;
            left: 2px;
            border: 2px solid #000000;
            border-top: none;
            border-right: none;
            background: rgba(255,255,255,0);
            transform: rotate(-45deg);
            transition: all 0.1s;
        }
        > span {
            position: relative;
            &:before {
                content: "";
                bottom: -1px;
                left: 0;
                width: 0;
                height: 1px;
                position: absolute;
                background-color: $color-body-text;
                transition: all 0.3s ease;
            }
        }
        &:hover {
            > span:before {
                width: 100%;
            }
        }
    }
    &.compare-added {
        input + label:after {
            opacity: 1;
            transform: scale(1) rotate(-45deg);
        }
    }
    @media (min-width: 1025px) {
        opacity: 0;
        right: 0;
        top: 50%;
        line-height: 1;
        display: block;
        visibility: hidden;
        position: absolute;
        pointer-events: none;
        transform: translateY(-50%);
        transition: all 0.3s ease;
        &:before {
            content: "";
            bottom: -4px;
            right: 0;
            left: auto;
            opacity: 0;
            height: 1px;
            position: absolute;
            display: inline-block;
            width: calc(100% - 21px);
            background-color: $color-body-text;
            transition: all 0.3s ease;
        }
        &:hover {
            &:before {
                opacity: 1;
            }
        }
    }
}

.product-card__variant {
    display: block;
    font-size: 0;
    line-height: 1;
    margin-bottom: 18px;
    &:last-child {
        margin-bottom: 7px;
    }
}

.product-card__variant__label {
    display: none;
    font-weight: 400;
    font-size: $font-size-base;
    color: $color-heading-text;
    margin-bottom: 12px;
    text-transform: capitalize;
}

.product-card__variant--size {
    font-size: 12px;
    text-transform: uppercase;
}

.product-card__variant--item {
    font-size: 0;
    margin-right: 8px;
    vertical-align: top;
    display: inline-block;
}

.product-card__variant--item--last {
    font-size: $font-size-base;
}

.product-card__variant--item label {
    width: 22px;
    height: 22px;
    margin: 0;
    border-radius: 50%;
    cursor: pointer;
    pointer-events: auto;
    background-size: cover;
    background-repeat: no-repeat;
    background-position: center;
}

.product-card__variant--item label.active {
    box-shadow: 0 0 0px 2px rgba(0,0,0,0.80);
}

.product-card__variant--item.white label {
    border: 1px solid #dadada;
}

.product-card__variant--item--last a {
    line-height: 22px;
    height: 22px;
    color: $color-body-text;
}

.product-card__button--wrapper {
    left: 0;
    right: 0;
    bottom: 100%;
    z-index: 3;
    display: block;
    position: absolute;
    transition: all 0.3s ease;
    &.is-open {
        height: auto;
        z-index: 3;
        bottom: 100%;
        opacity: 1;
        overflow: visible;
    }
    @media (min-width: 1025px) {
        z-index: 1;
        top: -45px;
        bottom: auto;
        height: 45px;
        opacity: 1;
        overflow: hidden;
        transition: all 0.3s ease, z-index 0s ease;
    }
}

.product-card_wishlist {
    display: block;
    text-align: center;
    width: 30px;
    height: 30px;
    padding: 5px;
    margin-left: auto;
    margin-right: 15px;
    margin-bottom: 15px;
    line-height: 20px;
    border-radius: 50%;
    background-color: #ffffff;
    &.wishlist-added {
        background-color: lighten($color-btn-checkout-bg, 14%);
        .text,
        .icon {
            background-color: lighten($color-btn-checkout-bg, 14%);
        }
    }
    .text {
        display: none;
        white-space: nowrap;
        font-size: 12px;
        font-weight: 400;
        width: 0;
        opacity: 0;
        overflow: hidden;
        visibility: hidden;
        right: calc(100% - 25px);
        height: 30px;
        background-color: #ffffff;
        line-height: 30px;
        position: absolute;
        top: 50%;
        padding: 0 30px 0 15px;
        border-radius: 15px;
        transform: translateY(-50%);
        transition: all 0.3s ease;
    }
    .icon {
        width: 20px;
        height: 20px;
        line-height: 20px;
        vertical-align: top;
        position: relative;
        z-index: 5;
        background-color: #ffffff;
    }
    @media (min-width: 1025px) {
        .text {
            display: inline-block;
        }
        &:hover {
            .text {
                width: auto;
                opacity: 1;
                visibility: visible;
                transition: all 0s ease;
            }
        }
    }
}

.product-card_quickview {
    display: none;
    @media (min-width: 1025px) {
        display: block;
        font-size: 14px;
        line-height: 30px;
        font-weight: 400;
        text-align: center;
        background-color: rgba(255,255,255,0.8);
    }
}

.product-card__button {
    display: block;
    position: relative;
    transition: all 0.3s ease;
}

.product-card__btn {
    margin: 0;
    width: 100%;
    display: block;
    font-size: 16px;
    line-height: 22px;
    font-weight: 500;
    padding: 8px 10px;
    text-align: center;
    white-space: nowrap;
    text-transform: capitalize;
    font-family: $font-stack-heading;
    color: $color-product-btn-text;
    border: 1px solid $color-product-btn-border;
    background-color: $color-product-btn-background;
    transition: all 0.3s ease;
    white-space: nowrap;
    overflow: hidden;
    text-overflow: ellipsis;
    span {
        position: relative;
    }
    .icon:not(.icon-spinner) {
        top: 3px;
        left: -20px;
        width: 11px;
        height: 11px;
        position: absolute;
    }
    [data-loader] {
        width: 20px;
        height: 20px;
    }
    [data-loader]:not(.hide) + [data-add-to-cart-text] {
        display: none;
    }
    [data-add-to-cart-text].hide + [data-loader] {
        display: inline-block !important;
    }
    &:hover {
        color: $color-product-btn-text;
        border-color: $color-product-btn-focus;
        background-color: $color-product-btn-focus;
    }
    &.product-card__btn-soldOut {
        border-color: lighten($color-product-btn-border, 70%);
        background-color: lighten($color-product-btn-background, 70%);
    }
    @media (min-width: 1025px) {
        padding: 10px;
    }
}

.product-card__btn--plus > span {
    position: relative;
    &:before {
        top: 8px;
        content: "";
        width: 11px;
        height: 1px;
        line-height: 22px;
        right: calc(100% + 8px);
        display: block;
        position: absolute;
        background-color: $color-product-btn-text;
    }
    &:after {
        top: 50%;
        content: "";
        width: 1px;
        height: 11px;
        line-height: 22px;
        right: calc(100% + 13px);
        display: block;
        position: absolute;
        background-color: $color-product-btn-text;
        transform: translateY(-50%);
    }
}

.product-card__variant--popup {
    top: calc(100% - 40px);
    left: 0;
    right: 0;
    z-index: 5;
    width: 100%;
    height: 0;
    opacity: 0;
    display: block;
    overflow: hidden;
    position: absolute;
    background-color: rgba(255,255,255,0.9);
    transition: all 0.3s ease;
    .is-open & {
        height: calc(100% + 56px);
        opacity: 1;
        overflow: visible;
    }
    .close {
        top: 0;
        right: 0;
        z-index: 6;
        opacity: 1;
        float: none;
        padding: 0 10px;
        font-size: 28px;
        font-weight: 400;
        line-height: 38px;
        position: absolute;
        color: #ffffff;
    }
    @media (min-width: 1025px) {
        top: calc(100% - 44px);
        .is-open & {
            height: calc(100% + 60px);
        }
        .close {
            line-height: 42px;
        }
    }
}

.product-card__variant--popup--content {
    width: 100%;
    text-align: center;
    display: inline-block;
    vertical-align: middle;
    max-height: 100%;
    .product-card__button2 {
        z-index: 6;
        display: block;
        transition: all 0.3s ease;
    }
    .product-form__variants {
        width: 100%;
    }
}

.product-card__Select {
    width: 100%;
    display: block;
    overflow: auto;
    height: 100px;
    padding: 10px 0 0 0;
    background-color: $color-body;
    &::-webkit-scrollbar-track {
        background-color: #f5f5f5;
    }
    &::-webkit-scrollbar {
        width: 5px;
        height: 5px;
        background-color: #f5f5f5;
    }
    &::-webkit-scrollbar-thumb {
        background-color: #a6a6a6;
    }
