/*================ Product Card ================*/

[data-slick-pc] {
    .product-card {
        width: 220px;
    }
    &.slick-slider {
        .product-card {
            width: 100%;
        }
    }
    @media (min-width: 768px) {
        .product-card {
            width: 250px;
        }
    }
    @media (min-width: 1025px) {
        .product-card {
            width: auto;
        }
    }
}

.product-card {
    position: relative;
    font-size: $font-size-base;
    background-color: $color-body;
    &:hover {
        .product-card_compare {
            opacity: 1;
            visibility: visible;
            pointer-events: auto;
        }
        .product-card__button--wrapper {
            z-index: 3;
            top: auto;
            bottom: 100%;
            height: auto;
            opacity: 1;
            overflow: visible;
            transition: all 0.3s ease, z-index 1.5s ease;
        }
        @media (min-width: 1025px) {
            .product-card__switchImage {
                .product-card__img {
                    opacity: 0;
                    visibility: hidden;
                }
                .product-card__img--2 {
                    opacity: 1;
                    visibility: visible;
                }
            }
        }
    }
    .halo-section-background-gray & {
        background-color: $color-section-bg-gray;
    }
}

.product-card--2 {
    display: table;
    width: 100%;
    .product-card__image {
        display: table-cell;
        vertical-align: top;
        width: 80px;
    }
    .product-card__content {
        display: table-cell;
        vertical-align: top;
        padding: 0 0 0 15px;
        width: calc(100% - 80px);
    }
    .product-card__title--variant {
        padding-right: 0;
    }
    .product-card__variant--item label {
        width: 16px;
        height: 16px;
        line-height: 16px;
    }
    .product-card__variant--item--last a {
        height: 16px;
        line-height: 16px;
    }
    .product-card__button {
        width: 120px;
        top: auto;
        left: auto;
        right: auto;
        bottom: auto;
        display: block;
        margin-top: 20px;
        position: relative;
    }
    @media (min-width: 551px) {
        .product-card__image {
            width: 90px;
        }

        .product-card__content {
            width: calc(100% - 90px);
        }
    }
    @media (min-width: 1025px) {
        .product-card__image {
            width: 100px;
        }

        .product-card__content {
            padding: 8px 0 0 20px;
            width: calc(100% - 100px);
        }
    }
}

.product-card--3 {
    width: auto;
    .product-card_wishlist {
        top: 15px;
        right: 15px;
        margin: 0;
        z-index: 10;
        position: absolute;
    }
    .product-card__image {
        max-width: 200px;
        margin-left: auto;
        margin-right: auto;
    }
    .product-card__button--wrapper {
        top: auto;
        left: auto;
        right: auto;
        bottom: auto;
        display: block;
        height: auto;
        opacity: 1;
        overflow: visible;
        visibility: visible;
        position: relative;
        margin-top: 20px;
    }
}

.product-card--4 {
    width: 280px;
    display: table;
    .product-card__image,
    .product-card__content--wrapper {
        display: table-cell;
        vertical-align: top;
    }
    .product-card__image {
        width: 80px;
    }
    .product-card__content {
        padding: 0;
    }
    .product-card__content--wrapper {
        width: calc(100% - 80px);
        padding: 10px;
    }
    .product-card__button--wrapper {
        display: none;
    }
    .product-card__variant {
        margin-bottom: 0;
        + .product-card__variant {
            display: none;
        }
    }
    @media (min-width: 768px) {
        width: 215px;
        display: block;
        .product-card__image,
        .product-card__content--wrapper {
            padding: 0;
            width: 100%;
            display: block;
        }
        .product-card__content {
            padding: 15px 20px;
        }
        .product-card__button--wrapper {
            z-index: 3;
            top: auto;
            bottom: 100%;
            height: auto;
            opacity: 1;
            display: block;
            overflow: visible;
        }
        .product-card__title a {
            display: -webkit-box;
            -webkit-line-clamp: 2;
            text-overflow: ellipsis;
            -webkit-box-orient: vertical;
        }
        .product-card__variant {
            margin-bottom: 18px;
            + .product-card__variant {
                display: block;
                margin-bottom: 0;
            }
        }
    }
}

