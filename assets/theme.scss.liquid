        justify-content: space-between;
    }
    .active {
        background-color: #f5f5f5;
    }
    [disabled="disabled"] {
        opacity: 0.5;
        pointer-events: none;
    }
    .text {
        text-align: left;
        padding-right: 10px;
    }
